<div class="post_group">
    <ul class="list_post">
        <% for(let post of posts) { %>
        <li class="post">
            <a href="/post" class="link_post">
                <span class="text_box">
                    <form action="" method="GET" id="post_frm">
                        <span class="post_info"><a href="#" onclick="chk_form( <%=post.post_id%> )" ><%=post.post_id%> <%= post.title %> <%= post.date %></a></span>
                    </form>
                </span>
            </a>
        </li>
        <br>
        <% } %>
    </ul>
</div>